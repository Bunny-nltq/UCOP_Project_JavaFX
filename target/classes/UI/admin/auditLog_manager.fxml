<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.ucop.controller.admin.AuditLogController">

    <TableView fx:id="table" layoutX="20" layoutY="20" prefWidth="700" prefHeight="400">
        <columns>
            <TableColumn fx:id="colEntity" text="Entity" prefWidth="120"/>
            <TableColumn fx:id="colEntityId" text="ID" prefWidth="70"/>
            <TableColumn fx:id="colAction" text="Action" prefWidth="100"/>
            <TableColumn fx:id="colActor" text="User" prefWidth="150"/>
            <TableColumn fx:id="colTime" text="Timestamp" prefWidth="250"/>
        </columns>
    </TableView>

</AnchorPane>
